\n      disabled: true\n    }\n  },\n  methods: {\n    agree () {\n      this.isUncheck = !this.isUncheck\n      this.disabled = !this.disabled\n      // if (this.isUncheck) {\n      //   this.isUncheck = false\n      //   this.disabled = false\n      // } else {\n      //   this.isUncheck = true\n      //   this.disabled = true\n      // }\n    },\n    goBack () {\n      this.$router.push({name: 'ChooseDate',params: {id: this.productId}})\n    },\n    selectedPayWay(wayId) {\n      this.payWay = wayId\n    },\n    closePopup() {\n      this.popupVisiblePayWays = false\n    },\n    checkPhoneNum(value) {\n      return /^(1[0-9][0-9]|14[57]|15[012356789]|17[0678]|18[0-9])\\d{8}$/.test(value)\n    },\n    createOrder() {\n      if (!this.orderNo) {\n        this.contactPersonState = this.contactPerson === \"\" ? 'error' : 'success'\n        this.contactPhoneState = this.checkPhoneNum(this.contactPhone) ? 'success' : 'error'\n        if (this.contactPersonState === 'error' || this.contactPhoneState === 'error') {\n          return\n        }\n        if (this.$store.state.lineUser.id) {\n          createOrder(this.stockGoodsId, this.adult, this.child, this.orderMoney, this.$store.state.lineUser.id, this.contactPerson, this.contactPhone).then(\n            res => {\n              if (res.data.state === 1) {\n                // this.MessageBox('提示', res.data.message);\n                this.orderNo = res.data.data.orderNo\n                this.popupVisiblePayWays = true\n                //如果创建成果跳转支付页面\n              } else {\n                // MessageBox('提示', res.data.message)\n              }\n            }\n          )\n        } else {\n          this.$router.push({name: 'Login'})\n        }\n      } else {\n        this.popupVisiblePayWays = true\n      }\n    },\n    _closeOrder () {\n      this.popupVisiblePayWays = false\n      let ua = navigator.userAgent.toLowerCase()\n      let result = ua.match(/MicroMessenger/i) || ''\n      if (!result) {\n        this.popupVisiblePayWait = true\n      }\n    }\n    // payThisOrder () {\n    //   payOrder(this.orderNo).then(\n    //     res => {\n    //       if (res.data.state === 1) {\n    //         MessageBox('提示', res.data.message)\n    //       } else {\n    //         // MessageBox('提示', res.data.message)\n    //       }\n    //     }\n    //   )\n    // }\n  },\n  created () {\n    let product = getStore('BOOK-PRODUCT')\n    if (product) {\n      product = JSON.parse(product)\n    }\n    let bookInfo = this.$route.query\n    // debugger;\n    // this.data = product.assign(bookInfo);\n    this.stockGoodsId = bookInfo.stockGoodsId\n    this.stockGoodsName = bookInfo.stockGoodsName\n    this.adult = bookInfo.adult\n    this.child = bookInfo.child\n    this.orderMoney = bookInfo.orderMoney\n    this.day = bookInfo.day\n    this.productId = product.productId\n    this.productName = product.productName\n    this.img = product.img\n    this.sellerName = product.sellerName\n  }\n}\n\n\n\n// WEBPACK FOOTER //\n// ./src/pages/order-confirmation.vue","var render = function () {var _vm=this;var _h=_vm.$createElement;var _c=_vm._self._c||_h;return _c('div',[_c('header',{staticClass:\"mint-header is-fixed\"},[_c('div',{staticClass:\"mint-header-button is-left\"},[_c('div',{},[_c('button',{staticClass:\"mint-button mint-button--default mint-button--normal\",on:{\"click\":_vm.goBack}},[_vm._m(0),_vm._v(\" \"),_c('label',{staticClass:\"mint-button-text\"})])])]),_vm._v(\" \"),_c('h1',{staticClass:\"mint-header-title\"},[_vm._v(\"确认订单\")]),_vm._v(\" \"),_c('div',{staticClass:\"mint-header-button is-right\"})]),_vm._v(\" \"),_c('div',{staticClass:\"content\"},[_c('div',{staticClass:\"content-cell\"},[_c('div',{staticClass:\"cell-title\"},[_vm._v(\"\\n        预订信息\\n      \")]),_vm._v(\" \"),_c('div',{staticClass:\"cell-body\"},[_c('img',{staticClass:\"w-40\",attrs:{\"src\":_vm.img}}),_vm._v(\" \"),_c('div',{staticClass:\"w-60\"},[_c('div',{staticClass:\"product-name\"},[_vm._v(_vm._s(_vm.product