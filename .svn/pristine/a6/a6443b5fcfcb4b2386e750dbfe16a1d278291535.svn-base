Order(this.$store.state.lineUser.id, orderNo).then(res => {\n        this.MessageBox('提示', res.data.message);\n        if (res && res.data.state === 1) {\n          this.getData();\n        }\n      })\n    },\n    getData(){\n      if (this.$store.state.lineUser.id) {\n        getOrderDetail(this.$store.state.lineUser.id, this.orderNo).then(res => {\n          if (res.data.state === 1) {\n            // this.MessageBox('提示', res.data.message);\n            this.order = res.data.data;\n            // this.order.orderMoney = this.order.orderMoney + '';\n            let orderStatusInfo = res.data.data.orderStatusInfo;\n\n            this.timeList = orderStatusInfo.map(d => ({\n              content: this.orderStateCfg[d.orderState].name,\n              theTime: d.createTime,\n              icon: this.orderStateCfg[d.orderState].icon\n            }))\n            this.timeList.push({\n              content: '订单提交成功',\n              theTime: this.order.orderTime,\n              icon: '√'\n            })\n\n            //如果创建成果跳转支付页面\n          } else {\n            this.MessageBox('错误', res.data.message);\n          }\n        })\n      }\n    }\n  },\n  created() {\n    if (this.$route.query.id) {\n      this.orderNo = this.$route.query.id;\n      this.getData();\n    }\n  }\n}\n\n\n\n// WEBPACK FOOTER //\n// ./src/pages/order-details.vue","var render = function () {var _vm=this;var _h=_vm.$createElement;var _c=_vm._self._c||_h;return _c('div',[_c('header',{staticClass:\"mint-header is-fixed\"},[_c('div',{staticClass:\"mint-header-button is-left\"},[_c('div',{},[_c('button',{staticClass:\"mint-button mint-button--default mint-button--normal\",on:{\"click\":_vm.goBack}},[_vm._m(0),_vm._v(\" \"),_c('label',{staticClass:\"mint-button-text\"})])])]),_vm._v(\" \"),_c('h1',{staticClass:\"mint-header-title\"},[_vm._v(\"订单详情\")]),_vm._v(\" \"),_c('div',{staticClass:\"mint-header-button is-right\"})]),_vm._v(\" \"),_c('div',{staticClass:\"content\"},[_c('div',{staticClass:\"con