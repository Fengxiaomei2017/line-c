'排序'\n      }],\n      themeList: [],\n      daysList: [],\n      wayList: [],\n      sortList: [{\n        name: '综合排序', cellKey: 'all'\n      }, {\n        name: '受欢迎程度', cellKey: 'views-desc'\n      }, {\n        name: '价格最高', cellKey: 'price-desc'\n      }, {\n        name: '价格最低', cellKey: 'price-asc'\n      },\n      {\n        name: '评分最高', cellKey: 'comments-desc'\n      }],\n      roadList: [],\n      storageNum: 1,\n      listScrollTop: ''\n    }\n  },\n  created () {\n    //pageNum, pageSize,days,type,theme,orderBy,orderDesc\n    this.listScrollTop = this.$store.state.listScrollTop\n    this.getRoadListData()\n    // this.getData(false, this.sentDays, this.sentType, this.sentTheme)\n  },\n  mounted () {\n    this.$nextTick(() => {\n      if (this.listScrollTop) {\n        window.scrollTo(0, this.listScrollTop)\n      }\n      document.addEventListener('touchstart', this.handleScroll)\n    })\n  },\n  destroyed () {\n    document.removeEventListener('touchstart', this.handleScroll)\n  },\n  methods: {\n    ...mapMutations([\n      'LIST_SCROLL_TOP'\n    ]),\n    goBack () {\n      this.$router.push('/home')\n    },\n    handleScroll () {\n      let scrollT = document.querySelector('.content').scrollTop\n      console.log(scrollT)\n      this.LIST_SCROLL_TOP(scrollT)\n    },\n    // handleScroll () {\n    //   // var scrollTop = window.pageYOffset || document.documentElement.scrollTop || document.body.scrollTop\n    //   let dayNum, parentScrollT, childOffsetT\n    //   if (this.roadList.length > 0) {\n    //     dayNum = Math.floor(this.roadList.length / 2)\n    //     parentScrollT = document.querySelector('#dayListJs').scrollTop\n    //     childOffsetT = document.getElementsByClassName('singleDayJs')[dayNum].offsetTop\n    //   }\n    //   if (parentScrollT > childOffsetT) {\n    //     if (this.total !== 0 && this.roadList.length < this.total) {\n    //       this.pageNum = this.pageNum + 1\n    //       this.getData(true, this.sentDays, this.sentType, this.sentTheme)\n    //     }\n    //   }\n    // },\n    getMoreBypage () {\n      if (this.total !== 0 && this.roadList.length < this.total) {\n        // console.log('长度：' + this.roadList.length)\n        this.loading = true\n        let _total = parseInt((this.total + '').slice(0, 1))\n        if (this.pageNum < _total) {\n          this.pageNum = this.pageNum + 1\n          this.getData(true, this.sentDays, this.sentType, this.sentTheme)\n        }\n        // this.pageNum = this.pageNum + 1\n      }\n    },\n    showFilter (id) {\n      this.isShowFilter = true\n      this.selected = id\n    },\n    hidFilter () {\n      this.isShowFilter = false\n      this.selected = ''\n    },\n    refreshList (type, cellKey) {\n      // debugger;\n      switch (type) {\n        case 'sortList':\n          if (cellKey === 'all') {\n            this.orderBy = ''\n            this.orderDesc = ''\n          } else {\n            this.orderBy = cellKey.split('-')[0]\n            this.orderDesc = cellKey.split('-')[1]\n          }\n          this.cellSelectedSort = cellKey\n          break\n        case 'themeList':\n          if (cellKey === 'all') {\n            this.cellSelectedTheme = 'all'\n          } else {\n            this.cellSelectedTheme = this.themeList[cellKey - 1].name\n          }\n          break\n        case 'daysList':\n          if (cellKey === 'all') {\n            this.cellSelectedDays = 'all'\n          } else {\n            this.cellSelectedDays = cellKey\n          }\n          break\n        case 'wayList':\n          if (cellKey === 'all') {\n            this.cellSelectedWay = 'all'\n          } else {\n            this.cellSelectedWay = cellKey\n          }\n          break\n        }\n      this.pageNum = 1\n      this.isShowFilter = false\n      this.sentDays = this.cellSelectedDays === 'all' ? '' : this.cellSelectedDays,\n      this.sentType = this.cellSelectedWay === 'all' ? '' : this.cellSelectedWay,\n      this.sentTheme = this.cellSelectedTheme === 'all' ? '' : this.cellSelectedTheme\n      this.getData(false, this.sentDays, this.sentType, this.sentTheme)\n    },\n    getData (isPush, days, type, theme) {\n      getRoadList(this.pageNum, this.pageSize, days, type, theme, this.orderBy, this.orderDesc).then(res => {\n        if (res && res.data.state === 1 && res.data.data.list) {\n          // this.roadList = res.data.data.list;\n          if (!isPush) {\n            this.total = res.data.data.total\n          }\n          let roadList = res.data.data.list.map(d => ({\n            days: d.DAYS,\n            price: d.MIN_PRICE,\n            pic: d.IMAGE,\n            startingPoint: d.START_ADDRESS,\n            id: d.PRODUCT_ID,\n            describe: d.PRODUCT_NAME\n          }))\n          this.roadList = isPush ? this.roadList.concat(roadList) : roadList\n          // this.roadList = roadList\n          this.loading = false\n        }\n      })\n    },\n    getRoadListData () {\n      getRoadListAndParam(this.pageNum, this.pageSize).then(resList => {\n        // debugger;\n        let res = resList[0]\n        let fliters = resList[1]\n        if (res && res.data.state === 1 && res.data.data.list) {\n          // this.roadList = res.data.data.list;\n          this.total = res.data.data.total\n          this.roadList = res.data.data.list.map(d => ({\n            days: d.DAYS,\n            price: d.MIN_PRICE,\n            pic: d.IMAGE,\n            startingPoint: d.START_ADDRESS,\n            id: d.PRODUCT_ID,\n            describe: d.PRODUCT_NAME\n          }))\n        }\n        if (fliters && fliters.data.state === 1) {\n          this.daysList = fliters.data.data.daysList.map((d, index) => ({\n            name: d,\n            cellKey: index + 1\n          }))\n          this.themeList = fliters.data.data.themeList.map((d, index) => ({\n            name: d,\n            cellKey: index + 1\n          }))\n          this.wayList = fliters.data.data.typeList.map((d) => ({\n            name: d.NAME,\n            cellKey: d.ID\n          }))\n        }\n      })\n    }\n  }\n}\n\n\n\n// WEBPACK FOOTER //\n// ./src/pages/list.vue","var render = function () {var _vm=this;var _h=_vm.$createElement;var _c=_vm._self._c||_h;return _c('div',[_c('header',{staticClass:\"mint-header is-fixed\"},[_c('a',{staticClass:\"mint-header-button is-left\",on:{\"click\":_vm.goBack}},[_c('i',{staticClass:\"mintui mintui-back\"})]),_vm._v(\" \"),_c('span',{staticClass:\"mint-header\"},[_vm._v(\"线路列表\")]),_vm._v(\" \"),_vm._m(0)]),_vm._v(\" \"),_c('Tab',{attrs:{\"tab-list\":_vm.tablist,\"selected\":_vm.selected,\"dropdown\":true},on:{\"selectedTab\":_vm.showFilter}}),_vm._v(\" \"),_c('TabContainer',{class:{'d-none':!_vm.isShowFilter}},[_c('TabContainerItem',{directives:[{name:\"show\",rawName:\"v-show\",value:(_vm.selected==='theme'),expression:\"selected==='theme'\"}],attrs:{\"id\":\"theme\"}},[_c('div',{staticClass:\"filter\"},[_c('button',{staticClass:\"btn btn-sm\",class:{'btn-yellow':_vm.cellSelectedTheme==='all'},attrs:{\"type\":\"button\"},on:{\"click\":function($event){_vm.refreshList('themeList','all')}}},[_vm._v(\"全部\\n          \")]),_vm._v(\" \"),_vm._l((_vm.themeList),function(item,themIndex){return _c('button',{staticClass:\"btn btn-sm\",class:{'btn-yellow':_vm.cellSelectedTheme===_vm.themeList[themIndex].name},attrs:{\"type\":\"button\"},on:{\"click\":function($event){_vm.refreshList('themeList',item.cellKey)}}},[_vm._v(\"\\n                  \"+_vm._s(item.name)+\"\\n          \")])})],2)]),_vm._v(\" \"),_c('TabContainerItem',{directives:[{name:\"show\",rawName:\"v-show\",value:(_vm.selected==='days'),expression:\"selected==='days'\"}],attrs:{\"id\":\"days\"}},[_c('div',{staticClass:\"filter\"},[_c('button',{staticClass:\"btn btn-sm\",class:{'btn-yellow':_vm.cellSelectedDays==='all'},attrs:{\"type\":\"button\"},on:{\"click\":function($event){_vm.refreshList('daysList','all')}}},[_vm._v(\"全部\\n          \")]),_vm._v(\" \"),_vm._l((_vm.daysList),function(item){return _c('but